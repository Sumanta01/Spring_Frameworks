<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean name="St1" class="com.sumanta.primitive.Student">
		<property name="id">
			<value>1001</value>
		</property>

		<property name="name">
			<value>Sumanta</value>
		</property>

		<property name="address">
			<value>India</value>

		</property>

	</bean>
	<bean name="St2" class="com.sumanta.primitive.Student">
		<property name="id">
			<value>1002</value>
		</property>

		<property name="name">
			<value>Sens</value>
		</property>

		<property name="address">
			<value>USA</value>

		</property>

	</bean>

</beans>