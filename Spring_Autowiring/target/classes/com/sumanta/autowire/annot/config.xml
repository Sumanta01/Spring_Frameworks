<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- bean definitions here -->
	<context:annotation-config></context:annotation-config>

	<bean name="ad" class="com.sumanta.autowire.annot.Address">
		<property name="city" value="Bhubaneswar">
		</property>
		<property name="state" value="Odisha"></property>

	</bean>
	
	<bean name="ad2" class="com.sumanta.autowire.annot.Address">
		<property name="city" value="Noida">
		</property>
		<property name="state" value="Delhi"></property>

	</bean>

	
	<bean name="St" class="com.sumanta.autowire.annot.Student"></bean>
	
	


</beans>